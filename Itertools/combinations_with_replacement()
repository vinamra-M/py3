# Enter your code here. Read input from STDIN. Print output to STDOUT
from itertools import combinations_with_replacement
a = list(input().split(" "))
S = a[0]
L = list(S)
L.sort()
k = int(a[1])
b = list(combinations_with_replacement(L, k))
for i in range(len(b)):
    st = ""
    st = "".join(b[i])
    print(st)
