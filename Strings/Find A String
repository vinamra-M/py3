def count_substring(string, sub_string):
    a = len(string)
    b = len(sub_string)
    c = a-b+1
    d = 0
    for i in range(c):
        s = string[i:i+b]
        if s == sub_string:
            d = d + 1
        
        
    
    return d

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)
